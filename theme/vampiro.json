            ],
            "settings": {
                "foreground": "#c686a8"
            }
        },
        {
            "name": "HTML tags",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#c686a8"
            }
        },
        {
            "name": "CSS attribute parent selectors ('&')",
