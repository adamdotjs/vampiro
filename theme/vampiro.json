            "settings": {
                "foreground": "#54b4c7"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "unused.comment",
                "wildcard.comment"
            ],
            "settings": {
                "foreground": "#7A837A"
            }
