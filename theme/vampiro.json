			"settings": {
				"foreground": "#e198b9"
			}
		},
		{
			"name": "Language Built-ins",
			"scope": ["support"],
			"settings": {
				"foreground": "#84e1d1"
			}
		},
		{
			"name": "Built-in magic functions and constants",
			"scope": ["support.function.magic", "support.variable", "variable.other.predefined"],
