                "meta.selector"
            ],
            "settings": {
                "foreground": "#FF318C"
            }
        },
        {
            "name": "Language Built-ins",
            "scope": [
                "support"
            ],
            "settings": {
                "foreground": "#07C3F2"
            }
        },
        {
            "name": "Built-in magic functions and constants",
