            "settings": {
                "foreground": "#21D789"
            }
        },
        {
            "name": "Decorator Objects",
            "scope": [
                "meta.decorator variable.other.object"
            ],
            "settings": {
                "foreground": "#21D789"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "punctuation.definition.keyword"
            ],
            "settings": {
                "foreground": "#FF318C"
            }
